!!!
%html
  %head
    %title= yield :title
    = stylesheet_link_tag :all
    %script{:type => "text/javascript", :src => "http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"}
    = javascript_include_tag 'highcharts'
    = javascript_include_tag './themes/dark-blue'
    = csrf_meta_tag
  %body
    .wrapper
      .header
      .content
        %h1= yield :title
        = yield
      .footer
